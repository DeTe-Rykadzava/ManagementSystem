@page "/products"
@using Database.Interfaces
@using Database.Models.Product
@rendermode InteractiveServer

<div class="d-flex justify-content-around mb-5 ">
    @if (_orderProducts.Any())
    {
        <div class="d-flex text-end align-items-center gap-2 shadow-sm p-2">
            <p class="m-0">В заказе @_orderProducts.Count</p>
            <a class="btn btn-secondary">К заказу</a>
        </div>
    }
</div>


<div class="container">
    <div class="d-flex w-100 flex-column">
        <div class="d-flex p-3">
            <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
              <ol class="carousel-indicators">
                <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
              </ol>
              <div class="carousel-inner">
                <div class="carousel-item active">
                  <img class="d-block w-100" src="..." alt="First slide">
                </div>
                <div class="carousel-item">
                  <img class="d-block w-100" src="..." alt="Second slide">
                </div>
                <div class="carousel-item">
                  <img class="d-block w-100" src="..." alt="Third slide">
                </div>
              </div>
              <a class="carousel-control-prev" href="products/#carouselExampleIndicators" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
              </a>
              <a class="carousel-control-next" href="products/#carouselExampleIndicators" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
              </a>
            </div>
        </div>
        <div class="d-flex p-3">
            
        </div>
    </div>
    <div class="d-flex flex-wrap justify-content-center gap-4">
        @* @foreach (var product in _products) *@
        @* { *@
        <div class="rounded-3 d-flex p-4 shadow flex-column" style="max-width: 20rem; width: 20rem;" @onclick="(e => { _selectedProduct = _products.FirstOrDefault(x => x.Id == 1); })">
            <img class="h-auto" alt="Title" src="data:image/png;base64, "/>
            <div class="h-auto p-4 gap-3">
                <B class="fs-2">Title product</B>
                <p class="fs-5">
                    1800 руб. за шт.
                </p>
            </div>
            <div class="h-auto text-end btn-group gap-2">
                @if (!_orderProducts.Contains(1))
                {
                    <button class="btn btn-primary" @onclick="(e => AddToOrder(1))">To Order</button>
                }
                else
                {
                    <button class="btn btn-danger" @onclick="(e => RemoveFromOrder(1))">Delete From Order</button>
                }
                <button class="btn btn-secondary">To Basket</button>
            </div>
        </div>
        <div class="rounded-3 d-flex p-4 shadow flex-column" style="max-width: 20rem; width: 20rem;">
            <img class="h-auto" alt="Title" src="data:image/png;base64, "/>
            <div class="h-auto p-4 gap-3">
                <B class="fs-2">Title product</B>
                <p class="fs-5">
                    1800 руб. за шт.
                </p>
            </div>
            <div class="h-auto text-end btn-group gap-2">
                @if (!_orderProducts.Contains(2))
                {
                    <button class="btn btn-primary" @onclick="(e => AddToOrder(2))">To Order</button>
                }
                else
                {
                    <button class="btn btn-danger" @onclick="(e => RemoveFromOrder(2))">Delete From Order</button>
                }
                <button class="btn btn-secondary">To Basket</button>
            </div>
        </div>
        <div class="rounded-3 d-flex p-4 shadow flex-column" style="max-width: 20rem;">
            <img class="h-auto" alt="Title" src="data:image/png;base64, "/>
            <div class="h-auto p-4 gap-3">
                <B class="fs-2">Title product</B>
                <p class="fs-5">
                    1800 руб. за шт.
                </p>
            </div>
            <div class="h-auto text-end btn-group gap-2">
                <button class="btn btn-primary">To Order</button>
                <button class="btn btn-secondary">To Basket</button>
            </div>
        </div>
        @* } *@
    </div>
</div>

@code {
    [Inject] private IProductRepository ProductRepository { get; set; } = default!;
    private List<ProductModel> _products = new();

    private List<int> _orderProducts { get; } = new();

    private ProductModel? _selectedProduct = null;

    protected override async void OnInitialized()
    {
        _products = (await ProductRepository.GetProducts()).ToList();
    }

    private void AddToOrder(int productId)
    {
        _orderProducts.Add(productId);
        Console.WriteLine("add");
        StateHasChanged();
    }
    
    private void RemoveFromOrder(int productId)
    {
        var productInOrder = _orderProducts.FirstOrDefault(x => x == productId);
        _orderProducts.Remove(productInOrder);
        StateHasChanged();
    }
    
    private void ChangeSelectedProduct(int productId)
    {
        var product = _products.FirstOrDefault(x => x.Id == productId);
        if (product == null) return;
        _selectedProduct = product;
        StateHasChanged();
    }

}